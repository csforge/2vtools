  	<function-class>to._2v.tools.helper.BeanHelper</function-class>
  	<function-signature>java.lang.String dateFormat(java.lang.Object,int)</function-signature>
  	<!-- Usage: dateformat(target,type) 
  		 ,the 'type' is:  0:ISO_TIME, 1:ISO_DATETIME, 2:ISO_DATE, 3:SLASH_DATETIME
  					, 4:SLASH_DATE, 5:NO_SLASH_DATETIME, 6:NO_SLASH_DATE, 7:ISO_DATETIME_MM(MM-dd HH:mm) -->
  </function>
  
