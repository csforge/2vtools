  	<function-class>to._2v.tools.helper.BeanHelper</function-class>
  	<function-signature>java.lang.String format(java.lang.Object,java.lang.String)</function-signature>
  	<!-- Usage: format(target,type) 
  		 ,the 'type' is: date,datetime,decimal,rounded -->
  </function>
  
